@import url(http://fonts.googleapis.com/css?family=Trade+Winds);
@font-face {
  font-family: 'Trade Winds';
  font-style: normal;
  font-weight: 400;
  src: local('Trade Winds'), local('TradeWinds'), url(http://themes.googleusercontent.com/static/fonts/tradewinds/v3/P7L1H4TWVDNJivDGg_--QLrIa-7acMAeDBVuclsi6Gc.woff) format('woff');
}
$cream: #fef9e7;
$navy: #2c3e50;
$navbar-inverse-bg: $navy;
$navbar-inverse-link-color: $cream;
$display-font-family: "Trade Winds", cursive;
$btn-default-bg: $cream;
$btn-default-color: $navy;

@mixin logo-bg {
	background-image: url( <%= asset_path 'stream.svg' %> );
	background-color: rgba(53,148,220,.2);
	background-position: center;
	background-repeat: no-repeat;
}

@import 'bootstrap';

.jumbotron {
	@include logo-bg;	
 	p {color: $navy; font-weight: 400;}
	h1 {
		color: $navy;
		font-family: $display-font-family; 
	} 
}
.center {text-align: center;}
.navbar-brand {
	font-family: $display-font-family;
}
